     ] )
  ] ) ; # 
  fhir:description [ fhir:v "This operation gathers data from all sites (facilities) configured for the given provider for the given measure and measurement period. A MeasureReport is returned that will have evaluatedResources that reference the resource data that was gathered. Additionally, the MeasureReport will have contained OperationOutcome resources that provide any messages that are the result of validating the data gathered."] ; # 
  fhir:jurisdiction ( [
     fhir:coding ( [
       fhir:system [ fhir:v "urn:iso:std:iso:3166"^^xsd:anyURI ] ;
       fhir:code [ fhir:v "US" ]
     ] )
  ] ) ; # 
  fhir:code [ fhir:v "gather"] ; # 
  fhir:system [ fhir:v "true"^^xsd:boolean] ; # 
