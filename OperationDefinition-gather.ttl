     fhir:type [ fhir:v "string" ] ;
     fhir:searchType [ fhir:v "reference" ]
  ] [
     fhir:name [ fhir:v "period" ] ;
     fhir:use [ fhir:v "in" ] ;
     fhir:min [ fhir:v "1"^^xsd:integer ] ;
     fhir:max [ fhir:v "1" ] ;
