       ]
     ] [
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-directReferenceCode"^^xsd:anyURI ] ;
       fhir:value [
         a fhir:Coding ;
         fhir:system [ fhir:v "http://terminology.hl7.org/CodeSystem/medicationrequest-category"^^xsd:anyURI ] ;
         fhir:code [ fhir:v "discharge" ] ;
         fhir:display [ fhir:v "Discharge" ]
       ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "TJCOverallFHIR" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Non Elective Inpatient Encounter" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "// Translator issue with 1.4.9-SNAPSHOT: https://github.com/cqframework/clinical_quality_language/issues/511\ndefine \"Non Elective Inpatient Encounter\":\n  [\"Encounter\": \"Non-Elective Inpatient Encounter\"] NonElectiveEncounter\n                  		where Global.\"LengthInDays\"(NonElectiveEncounter.period) <= 120\n                  			and NonElectiveEncounter.period ends during day of \"Measurement Period\"" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "0"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "TJCOverallFHIR" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "All Stroke Encounter" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"All Stroke Encounter\":\n  \"Non Elective Inpatient Encounter\" NonElectiveEncounter\n                      where Global.PrincipalDiagnosis(NonElectiveEncounter).code in \"Hemorrhagic Stroke\"\n                        or Global.PrincipalDiagnosis(NonElectiveEncounter).code in \"Ischemic Stroke\"" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "1"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "TJCOverallFHIR" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Encounter with Principal Diagnosis and Age" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Encounter with Principal Diagnosis and Age\":\n  \"All Stroke Encounter\" AllStrokeEncounter\n                  		with [\"Patient\"] BirthDate\n                  			such that AgeInYearsAt(start of AllStrokeEncounter.period) >= 18" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "2"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "TJCOverallFHIR" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Ischemic Stroke Encounter" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Ischemic Stroke Encounter\":\n  \"Encounter with Principal Diagnosis and Age\" EncounterWithAge\n                  		where Global.PrincipalDiagnosis(EncounterWithAge).code in \"Ischemic Stroke\"" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "3"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "DischargedonAntithromboticTherapyQICore4" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Antithrombotic Therapy at Discharge" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Antithrombotic Therapy at Discharge\":\n  [\"MedicationRequest\": medication in \"Antithrombotic Therapy\"] Antithrombotic\n    where Antithrombotic.doNotPerform is not true\n      and exists ( Antithrombotic.category C\n          where FHIRHelpers.ToConcept ( C ) ~ Global.\"Community\"\n            or FHIRHelpers.ToConcept ( C ) ~ Global.\"Discharge\"\n      )\n      and Antithrombotic.status in { 'active', 'completed' }\n      and Antithrombotic.intent = 'order'" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "4"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "DischargedonAntithromboticTherapyQICore4" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Numerator" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Numerator\":\n  TJC.\"Ischemic Stroke Encounter\" IschemicStrokeEncounter\n      with \"Antithrombotic Therapy at Discharge\" DischargeAntithrombotic\n        such that DischargeAntithrombotic.authoredOn during IschemicStrokeEncounter.period" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "5"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "DischargedonAntithromboticTherapyQICore4" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Denominator" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Denominator\":\n  TJC.\"Ischemic Stroke Encounter\"" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "6"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "DischargedonAntithromboticTherapyQICore4" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Initial Population" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Initial Population\":\n  TJC.\"Encounter with Principal Diagnosis and Age\"" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "7"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "TJCOverallFHIR" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Ischemic Stroke Encounters with Discharge Disposition" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Ischemic Stroke Encounters with Discharge Disposition\":\n  \"Ischemic Stroke Encounter\" IschemicStrokeEncounter\n                  		where IschemicStrokeEncounter.hospitalization.dischargeDisposition in \"Discharge To Acute Care Facility\"\n                  			or IschemicStrokeEncounter.hospitalization.dischargeDisposition in \"Left Against Medical Advice\"\n                  			or IschemicStrokeEncounter.hospitalization.dischargeDisposition in \"Patient Expired\"\n                  			or IschemicStrokeEncounter.hospitalization.dischargeDisposition in \"Discharged to Home for Hospice Care\"\n                  			or IschemicStrokeEncounter.hospitalization.dischargeDisposition in \"Discharged to Health Care Facility for Hospice Care\"" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "8"^^xsd:integer ]
       ] ) ;
       fhir:url [ fhir:v "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition"^^xsd:anyURI ]
     ] [
       fhir:extension ( [
         fhir:url [ fhir:v "libraryName"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "TJCOverallFHIR" ]
       ] [
         fhir:url [ fhir:v "name"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "Intervention Comfort Measures" ]
       ] [
         fhir:url [ fhir:v "statement"^^xsd:anyURI ] ;
         fhir:value [ fhir:v "define \"Intervention Comfort Measures\":\n  ( [\"ServiceRequest\": \"Comfort Measures\"] P\n          where P.intent = 'order'\n      )\n        union ( [\"Procedure\": \"Comfort Measures\"] InterventionPerformed\n            where InterventionPerformed.status in { 'completed', 'in-progress' }\n        )" ]
       ] [
         fhir:url [ fhir:v "displaySequence"^^xsd:anyURI ] ;
