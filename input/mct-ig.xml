<?xml version="1.0" encoding="UTF-8"?>
<ImplementationGuide xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../input-cache/schemas/R5/fhir-single.xsd">
  <id value="cms.fhir.mct"/>
  <url value="http://cms.gov/fhir/mct/ImplementationGuide/cms.fhir.mct"/>
  <!-- This version will propagate to all artifacts unless the "propagate-version" extension is overridden -->
  <version value="0.1.0"/>
  <name value="CMSFHIRMCT"/>
  <title value="CMS FHIR Prototype Measure Calculation Tool IG"/>
  <status value="draft"/>
  <experimental value="false"/>
  <publisher value="HL7 International - [Some] Work Group"/>
  <contact>
    <telecom>
      <!-- Or whatever URL and/or email address(es) are appropriate -->
      <system value="url"/>
      <value value="http://hl7.org/Special/committees/[something]"/>
    </telecom>
  </contact>
  <description value="This implementation guide documents and supports the use of a prototype Measure Calculation Tool for reporting and calculating FHIR-based digital quality measures (dQMs)."/>
  <jurisdiction>
    <coding>
      <system value="urn:iso:std:iso:3166"/>
      <code value="US"/>
    </coding>
  </jurisdiction>  
  <packageId value="cms.fhir.mct" />
  <license value="Apache-2.0"/>
  <fhirVersion value="4.0.1"/>
  <dependsOn id="cqf">
    <uri value="http://fhir.org/guides/cqf/common/ImplementationGuide/fhir.cqf.common"/>
    <packageId value="fhir.cqf.common"/>
    <version value="4.0.1"/>
  </dependsOn>
  <dependsOn id="cpg">
    <uri value="http://hl7.org/fhir/uv/cpg/ImplementationGuide/hl7.fhir.uv.cpg" />
    <packageId value="hl7.fhir.uv.cpg" />
    <version value="1.0.0" />
  </dependsOn>
  <dependsOn id="qm">
    <uri value="http://hl7.org/fhir/us/cqfmeasures/ImplementationGuide/hl7.fhir.us.cqfmeasures" />
    <packageId value="hl7.fhir.us.cqfmeasures" />
    <version value="3.0.0" />
  </dependsOn>
  <dependsOn id="deqm">
    <uri value="http://hl7.org/fhir/us/davinci-deqm/ImplementationGuide/hl7.fhir.us.davinci-deqm"/>
    <packageId value="hl7.fhir.us.davinci-deqm"/>
    <version value="3.1.0"/>
  </dependsOn>
  <dependsOn id="uscore">
    <uri value="http://hl7.org/fhir/us/core/ImplementationGuide/hl7.fhir.us.core"/>
    <packageId value="hl7.fhir.us.core"/>
    <version value="3.1.1"/>
  </dependsOn>
  <dependsOn id="qicore">
    <uri value="http://hl7.org/fhir/us/qicore/ImplementationGuide/hl7.fhir.us.qicore"/>
    <packageId value="hl7.fhir.us.qicore"/>
    <version value="4.1.1"/>
  </dependsOn>
  <definition>
    <!-- Device -->
    <resource>
      <reference>
        <reference value="Device/cqf-tooling" />
        <display value="CQF Tooling Device" />
      </reference>
      <name value="CQFToolingDevice" />
      <description value="A Device that represents a CQF Tooling version." />
      <exampleBoolean value="false" />
    </resource>
    <!-- Examples -->
    <resource>
      <reference>
        <reference value="Endpoint/example" />
        <display value="Example Enpoint" />
      </reference>
      <name value="ExampleEndpoint" />
      <description value="An example endpoint for a receiving system." />
      <exampleBoolean value="true" />
    </resource>
    <resource>
      <reference>
        <reference value="Location/acme-north" />
        <display value="ACME North Example Location" />
      </reference>
      <name value="ACMENorthExampleLocation" />
      <description
        value="An example location for ACME organization with contained Endpoint resource." />
      <exampleBoolean value="true" />
    </resource>
    <resource>
      <reference>
        <reference value="Location/acme-south" />
        <display value="ACME South Example Location" />
      </reference>
      <name value="ACMESouthExampleLocation" />
      <description
        value="An example location for ACME organization with contained Endpoint resource." />
      <exampleBoolean value="true" />
    </resource>
    <resource>
      <reference>
        <reference value="Organization/acme" />
        <display value="Example Organization" />
      </reference>
      <name value="ExampleOrganization" />
      <description
        value="An example Organization resource." />
      <exampleBoolean value="true" />
    </resource>
    <!-- Extensions -->
    <resource>
      <reference>
        <reference value="StructureDefinition/mct-receivingSystemEndpoint" />
        <display value="Receiving System Endpoint Extension" />
      </reference>
      <name value="ReceivingSystemEndpointExtension" />
      <description
        value="Specifies the receiving system endpoint as a reference to an Endpoint resource" />
      <exampleBoolean value="false" />
    </resource>

    <page>
      <!-- The root will always be toc.html - the template will force it if you don't do it -->
      <nameUrl value="toc.html"/>
      <title value="Table of Contents"/>
      <generation value="html"/>
      <page>
        <nameUrl value="index.html"/>
        <title value="MCT IG Home Page"/>
        <generation value="html"/>
      </page>
      <page>
        <nameUrl value="background.html"/>
        <title value="Background"/>
        <generation value="html"/>
      </page>
      <page>
        <nameUrl value="technical-requirements.html"/>
        <title value="Technical Requirements"/>
        <generation value="markdown"/>
      </page>
      <page>
        <nameUrl value="implementation-guide.html"/>
        <title value="Implementation Guide"/>
        <generation value="markdown"/>
      </page>
      <page>
        <nameUrl value="user-guide.html"/>
        <title value="User Guide"/>
        <generation value="markdown"/>
      </page>
      <page>
        <nameUrl value="test-plan.html"/>
        <title value="Test Plan"/>
        <generation value="markdown"/>
      </page>
      <page>
        <nameUrl value="downloads.html"/>
        <title value="Useful Downloads"/>
        <generation value="html"/>
      </page>
      <page>
        <nameUrl value="license.html"/>
        <title value="License"/>
        <generation value="markdown"/>
      </page>
      <page>
        <nameUrl value="changes.html"/>
        <title value="IG Change History"/>
        <generation value="html"/>
      </page>
    </page>
    <!-- copyright year is a mandatory parameter -->
    <parameter>
      <code value="copyrightyear"/>
      <value value="2022+"/>
    </parameter>
    <!-- releaselabel should be the ballot status for HL7-published IGs. -->
    <parameter>
      <code value="releaselabel"/>
      <value value="CI Build"/>
    </parameter>
    <parameter>
      <code value="find-other-resources"/>
      <value value="true"/>
    </parameter>
    <!-- This parameter indicates to the publisher that it should translate and package CQL libraries. See the example library resources for more -->
    <!-- This is disabled for now because the CQF Tooling has more up-to-date CQL processing than the publisher. Run refresh to update IG resources
    <parameter>
      <code value="path-binary"/>
      <value value="input/cql"/>
    </parameter>
    -->
    <parameter>
      <code value="path-liquid"/>
      <value value="templates/liquid"/>
    </parameter>
    <parameter>
      <code value="path-suppressed-warnings"/>
      <value value="input/ignoreWarnings.txt"/>
    </parameter>
    <parameter>
      <code value="path-resource" />
      <value value="input/resources/device" />
    </parameter>
    <parameter>
      <code value="path-resource" />
      <value value="input/resources/library" />
    </parameter>
    <parameter>
      <code value="path-resource" />
      <value value="input/resources/measure" />
    </parameter>
    <!-- Uncomment one or more of these if you want to limit which syntaxes are supported or want to disable the display of mappings
    <parameter>
      <code value="excludexml"/>
      <value value="true"/>
    </parameter>
    <parameter>
      <code value="excludejson"/>
      <value value="true"/>
    </parameter>
    <parameter>
      <code value="excludettl"/>
      <value value="true"/>
    </parameter>
    <parameter>
      <code value="excludemap"/>
      <value value="true"/>
    </parameter>-->
  </definition>
</ImplementationGuide>
